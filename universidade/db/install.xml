<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/universidade/db" VERSION="20210818" COMMENT="XMLDB file for Moodle blocks/city"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="blocks_universidade" COMMENT="Table containing information for each univerity">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="universidade" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="franchised universidade"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
